<p id="notice"><%= notice %></p>

<p>
  <strong>Url:</strong>
  <%= @post.url %>
</p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Votes:</strong>
  <%= @post.votes %>
</p>
<%= link_to 'Upvote', post_upvote_path(@post, :post_id => @post.id) %>
<%= link_to 'Downvote', post_downvote_path(@post,:post_id => @post.id) %>
<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path(@post, :post_id => @post.id) %>
